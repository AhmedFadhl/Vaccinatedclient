@model Vaccinatedclient.Models.hospital_type

    <div class="main">
            <div class="main_top">
                <img src="/images/element223111-9f3.svg" alt="user">
                <h2>إضافة  نوع للمراكز والمستشفيات </h2>
            </div>

            <div class="main_bottom">
                <form enctype="multipart/form-data" asp-action="add_hospital_type">
                <div class="form_input">
                    <div class="main_det">
                 <div class="main_det_input">   
                    <div class="text_input">
                        <h3>رقم النوع</h3>
                        <input asp-for=ID disabled type="text">
                    </div>
                    <div class="text_input">
                        <h3>النوع</h3>
                        <input asp-for="type" type="text">
                    </div>
                    <div class="text_input">
                        <h3>الوصف</h3>
                        <input asp-for="desc" type="text">
                    </div>
                  </div>
                  </div>
                </div>
                    <div class="submit_buttons">
                        <input type="submit" name="حفظ" value="save">
                        <button>ألغاء</button>
                    </div>
                </form>
               
            </div>

        </div>

